{
    "name": "veterandb-backend",
    "version": "0.0.1",
    "description": "Veteran Database is the single resource for military Veterans.",
    "main": "server.ts",
    "scripts": {
        "coverage": "cross-env NODE_ENV=development nyc --reporter=text npm test",
        "db:help": "typeorm -h",
        "start:dev": "NODE_ENV=development nodemon --watch './**/*.ts' --exec 'ts-node' src/server.ts",
        "start:prod": "NODE_ENV=development nodemon --watch './**/*.ts' --exec 'ts-node' src/server.ts",
        "start:test": "NODE_ENV=development nodemon --watch './**/*.ts' --exec 'ts-node' src/server.ts",
        "style": "sass public/css/scss/style.scss:public/css/style.css",
        "style:min": "sass public/css/scss/style.scss:public/css/style.css --style=compressed",
        "style:watch": "sass --watch public/css/scss/style.scss:public/css/style.css",
        "test:ts": "NODE_ENV=development mocha --config test/_setup/mocha/.mocharc.jsonc",
        "webpack:dev": "webpack --config webpack.config.dev.js",
        "webpack:prod": "webpack --config webpack.config.prod.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/carbondigitalus/veterandb-backend.git"
    },
    "author": "Carbon Digital",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/carbondigitalus/veterandb-backend/issues"
    },
    "homepage": "https://github.com/carbondigitalus/veterandb-backend#readme",
    "devDependencies": {
        "chai": "^4.3.6",
        "chai-as-promised": "^7.1.1",
        "clean-webpack-plugin": "^4.0.0",
        "eslint": "^6.8.0",
        "eslint-config-airbnb": "^18.2.0",
        "eslint-config-prettier": "^6.13.0",
        "eslint-plugin-chai-expect": "^2.2.0",
        "eslint-plugin-chai-friendly": "^0.5.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jsx-a11y": "^6.3.1",
        "eslint-plugin-mocha": "^6.3.0",
        "eslint-plugin-node": "^10.0.0",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-react": "^7.21.4",
        "eslint-plugin-react-hooks": "^1.7.0",
        "mocha": "^9.2.2",
        "morgan": "^1.10.0",
        "nodemon": "^2.0.16",
        "prettier": "^1.19.1",
        "sinon": "^13.0.1",
        "sinon-chai": "^3.7.0",
        "supertest": "^6.2.2",
        "ts-mocha": "^9.0.2",
        "ts-node": "^10.7.0",
        "tslint": "^6.1.3",
        "webpack": "^5.61.0",
        "webpack-cli": "^4.9.1",
        "webpack-dev-server": "^4.4.0"
    },
    "dependencies": {
        "bcrypt": "^5.0.1",
        "class-validator": "^0.13.2",
        "compression": "^1.7.4",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-rate-limit": "^5.1.3",
        "helmet": "^5.0.2",
        "hpp": "^0.2.3",
        "html-to-text": "^8.1.0",
        "jquery": "^3.5.1",
        "jsonwebtoken": "^8.5.1",
        "multer": "^1.4.4",
        "mysql2": "^2.3.3",
        "nodemailer": "^6.7.2",
        "pug": "^3.0.2",
        "reflect-metadata": "^0.1.13",
        "sharp": "^0.30.2",
        "typeorm": "^0.2.45",
        "xss-clean": "^0.1.1"
    },
    "engines": {
        "node": ">=16.X"
    }
}
