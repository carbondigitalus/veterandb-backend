        "@types/chai": "^4.3.0",
        "@types/chai-as-promised": "^7.1.5",
        "@types/compression": "^1.7.2",
        "@types/cookie-parser": "^1.4.3",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/hpp": "^0.2.2",
        "@types/html-to-text": "^8.1.0",
        "@types/jquery": "^3.5.14",
        "@types/jsonwebtoken": "^8.5.8",
        "@types/mocha": "^9.1.0",
        "@types/morgan": "^1.9.3",
        "@types/multer": "^1.4.7",
        "@types/node": "^17.0.21",
        "@types/nodemailer": "^6.4.4",
        "@types/pug": "^2.0.6",
